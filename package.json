{
  "name": "thepuppeteer",
  "version": "0.1.0",
  "description": "A Vibe Kanban compliant agentic framework",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "thepuppeteer": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:server": "npx ts-node src/server/index.ts",
    "start:web": "npm run dev --prefix web",
    "test": "jest",
    "lint": "eslint src tests",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "docker:build": "docker build -t thepuppeteer .",
    "docker:run": "docker run -it thepuppeteer"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/swagger-ui-express": "^4.1.6",
    "autoprefixer": "^10.4.24",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "swagger-ui-express": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
